{
  "name": "coding-agent-template-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:data": "npx tsx scripts/build-data.ts",
    "build:data:firstchoice": "npx tsx scripts/build-first-choice.ts",
    "validate:firstchoice": "npx tsx scripts/validateFirstChoice.ts",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "typecheck": "npx tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@duckdb/node-api": "1.3.2-alpha.26",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,md}": "npm run format && npm run lint && npm run coverage"
  }
}
