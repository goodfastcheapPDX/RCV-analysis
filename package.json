{
  "name": "coding-agent-template-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:data": "npx tsx scripts/build-data.ts",
    "build:data:firstchoice": "npx tsx scripts/build-first-choice.ts",
    "validate:firstchoice": "npx tsx scripts/validateFirstChoice.ts",
    "start": "next start",
    "lint": "biome check --fix",
    "format": "biome format --write --fix",
    "typecheck": "npx tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@duckdb/node-api": "1.3.2-alpha.26",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.542.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/nextjs-vite": "^9.1.3",
    "@storybook/test-runner": "^0.23.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "storybook": "^9.1.3",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,md}": [
      "npm run format",
      "npm run lint"
    ]
  }
}
