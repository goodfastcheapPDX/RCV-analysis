{
  "name": "coding-agent-template-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:data": "npx tsx scripts/build-data.ts",
    "build:data:all": "npx tsx scripts/build-all-districts.ts",
    "build:data:firstchoice": "npx tsx scripts/build-first-choice.ts",
    "build:data:rankdist": "npx tsx scripts/build-rank-distribution.ts",
