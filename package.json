    "validate:firstchoice": " npx tsx scripts/validateFirstChoice.ts",
    "validate:stv": "npx tsx scripts/validateStvRounds.ts",
    "start": "next start",
    "lint": "biome check --fix --no-errors-on-unmatched",
    "format": "biome format --write --fix --no-errors-on-unmatched",
    "typecheck": "npx tsc --noEmit",
    "test": "vitest --silent=true",
    "test:debug": "vitest",
    "coverage": "vitest run --coverage --silent=true",
    "coverage:debug": "vitest run --coverage",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
